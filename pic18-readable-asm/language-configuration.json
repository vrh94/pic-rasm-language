{
    "comments": {
        "lineComment": ";"
    },
    "brackets": [
        ["(", ")"]
    ],
    "autoClosingPairs": [
        { "open": "(", "close": ")" },
        { "open": "'", "close": "'", "notIn": ["string", "comment"] },
        { "open": "\"", "close": "\"", "notIn": ["string", "comment"] }
    ],
    "surroundingPairs": [
        ["(", ")"],
        ["'", "'"],
        ["\"", "\""]
    ],
    "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}
