{
    "name": "pic18-readable-asm",
    "displayName": "PIC16/PIC18 Readable Assembly",
    "description": "Syntax highlighting and autocomplete for PIC16/PIC18 readable assembly (.rasm) â€” English & Slovenian mnemonics",
    "version": "1.1.0",
    "publisher": "local",
    "engines": {
        "vscode": "^1.60.0"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:pic18rasm"
    ],
    "categories": ["Programming Languages"],
    "contributes": {
        "languages": [
            {
                "id": "pic18rasm",
                "aliases": ["PIC18 Readable Assembly", "rasm"],
                "extensions": [".rasm"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pic18rasm",
                "scopeName": "source.pic18rasm",
                "path": "./syntaxes/pic18rasm.tmLanguage.json"
            }
        ]
    }
}
